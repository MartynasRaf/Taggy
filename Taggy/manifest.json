  {
    "name": "Taggy",
    "version": "1.0",
    "description": "Bookmark webpages, add tags to them",
    "permissions": ["<all_urls>" , "tabs" , "identity" , "activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "images/LogoType.png"
      }
    },
	"options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
	"content_security_policy":"script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js; object-src 'self'",
    "icons": {
      "128": "images/LogoType.png"
    },
    "manifest_version": 2
  }